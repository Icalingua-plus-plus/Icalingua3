  "lint-staged": {
    "*.{js,jsx,ts,tsx,vue}": [
      "eslint --fix"
    ],
    "*.{json,md,scss,yaml,yml}": [
      "prettier -w"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
